{
    "name": "m0rtis/satis-webhook",
    "description": "Simple application for update satis repository through webhooks",
    "type": "project",
    "require": {
        "php": "^7.2",
        "composer/satis": "^1.0",
        "slim/slim": "^4.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Composer\\Satis\\Webhook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Composer\\Satis\\Webhook\\Test\\": "tests/"
        }
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "m0rtis",
            "email": "mail@m0rtis.ru"
        }
    ],
    "minimum-stability": "dev"
}
